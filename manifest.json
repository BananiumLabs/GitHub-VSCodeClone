{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GitHub Clone for Visual Studio Code",
  "short_name": "VSCode-gitclone",
  "description": "Overrides Open in Visual Studio button on GitHub with Open in Visual Studio Code",
  "version": "1.0.0",
  "version_name": "1.0.0alpha",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "https://github.com/*"
  ],
  "author": "Eric Qian",
  "homepage_url": "https://enumc.com",
  "manifest_version": 2
}
